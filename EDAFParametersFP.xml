<?xml version="1.0" encoding="UTF-8"?>
<EDAF>
    <Algorithm>
        <Name>pbil</Name>
    </Algorithm>
	<WorkEnvironment>
		<Name>coco.TEST</Name>
	</WorkEnvironment>
    <Genotype>
        <Gene>FP</Gene>
        <lBound>-5</lBound>
        <uBound>5</uBound>
        <Dimension>30</Dimension>
        <Precision>5</Precision>
        <Crossing key="simpleArithmetic">0.7</Crossing>
        <Mutation key="simple">0.05</Mutation>
    </Genotype>
    <Registry>
        <PopulationSize>50</PopulationSize>
        <EstimationProbability>0.5</EstimationProbability>
        <Elitism>1</Elitism>
		<Mortality>0.4</Mortality>
        <MaxNumberOfGen>10000</MaxNumberOfGen>
        <Stagnation>0</Stagnation>
		<LogFrequency>200</LogFrequency>
        <LogDirectory>.</LogDirectory>
        <Selection key="truncation" ratio="0.3">3</Selection>
    </Registry>
    <AlgorithmParam>
		<RatioSelector ratio="0.5">dynamic</RatioSelector>
    </AlgorithmParam>
</EDAF>